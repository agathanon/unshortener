{
  "manifest_version": 2,
  "name": "Unshorten",
  "version": "0.1",

  "description": "Unshorten links from Twitter.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
